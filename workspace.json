{
  "version": 2,
  "projects": {
    "adapters": {
      "root": "libs/adapters",
      "sourceRoot": "libs/adapters/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/adapters/**/*.js"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/adapters"],
          "options": {
            "jestConfig": "libs/adapters/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "exceptions": {
      "root": "libs/exceptions",
      "sourceRoot": "libs/exceptions/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/exceptions/**/*.js"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/exceptions"],
          "options": {
            "jestConfig": "libs/exceptions/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "helpers": {
      "root": "libs/helpers",
      "sourceRoot": "libs/helpers/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/helpers/**/*.js"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/helpers"],
          "options": {
            "jestConfig": "libs/helpers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "notes": {
      "root": "apps/notes",
      "sourceRoot": "apps/notes",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/notes",
            "main": "apps/notes/src/main.js",
            "tsConfig": "apps/notes/tsconfig.app.json"
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/notes",
            "commands": [
              "cross-env NODE_ENV=prod node esbuild.js",
              "npx cdk deploy --require-approval=never"
            ]
          }
        },
        "destroy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/notes",
            "command": "npx cdk destroy",
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/notes/**/*.js"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/notes"],
          "options": {
            "jestConfig": "apps/notes/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "notes"
}
